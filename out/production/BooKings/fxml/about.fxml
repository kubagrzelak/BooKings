<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane prefHeight="400" prefWidth="500" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1">
   <top>
      <ImageView fitWidth="500.0" pickOnBounds="true" preserveRatio="true" BorderPane.alignment="CENTER">
         <Image url="@../images/about-banner.png" />
      </ImageView>
   </top>
   <center>
      <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
         <Label contentDisplay="TOP" graphicTextGap="6.0" text="Version 1.0">
            <graphic>
               <ImageView fitWidth="55.0" pickOnBounds="true" preserveRatio="true">
                  <Image url="@../images/logo2.png" />
               </ImageView>
            </graphic>
            <VBox.margin>
               <Insets top="20.0" />
            </VBox.margin>
         </Label>
         <Label text="2020" />
         <Label text="Creators:">
            <VBox.margin>
               <Insets top="15.0" />
            </VBox.margin>
            <font>
               <Font size="14.0" />
            </font>
         </Label>
         <HBox alignment="TOP_CENTER" prefHeight="100.0" prefWidth="200.0" spacing="50.0">
            <VBox.margin>
               <Insets bottom="20.0" left="10.0" right="10.0" top="10.0" />
            </VBox.margin>
            <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0">
               <ImageView fitHeight="70.0" fitWidth="70.0" pickOnBounds="true" preserveRatio="true">
                  <Image url="@../images/Janek.JPG" />
                  <VBox.margin>
                     <Insets bottom="5.0" />
                  </VBox.margin>
               </ImageView>
               <Label text="Jan" />
               <Label text="Marczak" />
            </VBox>
            <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0">
               <ImageView fitHeight="70.0" fitWidth="70.0" pickOnBounds="true" preserveRatio="true">
                  <VBox.margin>
                     <Insets bottom="5.0" />
                  </VBox.margin>
                  <image>
                     <Image url="@../images/Matteo.jpg" />
                  </image></ImageView>
               <Label text="Matteo" />
               <Label text="Fortier" />
            </VBox>
            <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0">
               <ImageView fitHeight="70.0" fitWidth="70.0" pickOnBounds="true" preserveRatio="true">
                  <Image url="@../images/Kuba.png" />
                  <VBox.margin>
                     <Insets bottom="5.0" />
                  </VBox.margin></ImageView>
               <Label text="Jakub" />
               <Label text="Grzelak" />
            </VBox>
            <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0">
               <ImageView fitHeight="70.0" fitWidth="70.0" pickOnBounds="true" preserveRatio="true">
                  <VBox.margin>
                     <Insets bottom="5.0" />
                  </VBox.margin>
                  <image>
                     <Image url="@../images/Andris.jpg" />
                  </image></ImageView>
               <Label text="Andras" />
               <Label text="Horvath" />
            </VBox>
         </HBox>
      </VBox>
   </center>
   <stylesheets>
      <URL value="@../stylesheets/fonts.css" />
      <URL value="@../stylesheets/about.css" />
   </stylesheets>
</BorderPane>
